<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/5.4/window.xsd"
        class="com.haulmont.sampler.gui.containers.tabsheet.lazy.LazyTabSheetFrame">
    <layout>
        <hbox spacing="true">
            <tabSheet id="tabSheet"
                      width="200">
                <tab id="tab1"
                     lazy="true"
                     caption="Tab 1"
                     margin="true,false,false,true">
                    <label id="label1"
                           value="Label 1"/>
                </tab>
                <tab id="tab2"
                     lazy="true"
                     caption="Tab 2"
                     margin="true,false,false,true">
                    <label id="label2"
                           value="Label 2"/>
                </tab>
            </tabSheet>
            <label id="checkMessage" align="MIDDLE_LEFT"/>
        </hbox>
    </layout>
</window>
