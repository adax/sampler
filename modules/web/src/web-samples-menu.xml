<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<sample-menu-config xmlns="com/haulmont/sampler/gui/samples-menu.xsd">

    <menu id="components">
        <menu id="button">
            <item id="simple-button" docUrlSuffix="gui_Button"/>
            <item id="localized-button" docUrlSuffix="gui_Button"/>
            <item id="action-button" docUrlSuffix="gui_Button"/>
            <item id="programmatic-action-button" docUrlSuffix="gui_Button"/>
            <item id="link-button" docUrlSuffix="gui_Link"/>
        </menu>

        <item id="bulk-editor" docUrlSuffix="gui_BulkEditor"/>

        <menu id="checkBox">
            <item id="simple-checkbox" docUrlSuffix="gui_CheckBox"/>
            <item id="datasource-checkbox" docUrlSuffix="gui_CheckBox"
                  description="sample-config.datasource-checkbox.description"/>
        </menu>

        <menu id="textField">
            <item id="simple-textfield" docUrlSuffix="gui_TextField"/>
            <item id="datasource-textfield" docUrlSuffix="gui_TextField"
                  description="sample-config.datasource-datefield.description"/>
            <item id="datatype-textfield" docUrlSuffix="gui_TextField"/>
            <item id="validator-textfield" docUrlSuffix="gui_TextField"/>
            <item id="trim-textfield" docUrlSuffix="gui_TextField"/>
        </menu>

        <menu id="dateField">
            <item id="simple-datefield" docUrlSuffix="gui_DateField"/>
            <item id="datasource-datefield" docUrlSuffix="gui_DateField"
                  description="sample-config.datasource-datefield.description"/>
            <item id="format-datefield" docUrlSuffix="gui_DateField"/>
            <item id="resolution-datefield" docUrlSuffix="gui_DateField"/>
        </menu>

        <menu id="embedded">
            <item id="image-embedded" docUrlSuffix="gui_Embedded"/>
            <item id="web-embedded" docUrlSuffix="gui_Embedded"/>
        </menu>

        <menu id="fieldGroup">
            <item id="simple-fieldgroup" docUrlSuffix="gui_FieldGroup"/>
        </menu>

        <menu id="label">
            <item id="simple-label" docUrlSuffix="gui_Label"/>
            <item id="multiline-label" docUrlSuffix="gui_Label"/>
            <item id="dynamic-label" docUrlSuffix="gui_Label"/>
            <item id="datasource-label" docUrlSuffix="gui_Label"/>
            <item id="html-label" docUrlSuffix="gui_Label"/>
        </menu>

        <item id="simple-link" docUrlSuffix="gui_Link"/>

        <menu id="table">
            <item id="simple-table" docUrlSuffix="gui_Table"/>
            <item id="maxlength-table" docUrlSuffix="gui_Table"/>
            <item id="action-table" docUrlSuffix="gui_Table"/>
            <item id="editable-table" docUrlSuffix="gui_Table"/>
            <item id="formatter-table" docUrlSuffix="gui_Table"/>
            <item id="icon-table" docUrlSuffix="gui_Table"/>
            <item id="style-table" docUrlSuffix="gui_Table"/>
            <item id="itemclick-table" docUrlSuffix="gui_Table"/>
            <item id="enterpress-table" docUrlSuffix="gui_Table"/>
            <item id="generate-table" docUrlSuffix="gui_Table"/>
            <item id="aggregatable-table" docUrlSuffix="gui_Table"/>
            <item id="presentations-table" docUrlSuffix="gui_Table"/>
            <item id="group-table-browse" docUrlSuffix="gui_GroupTable"/>
            <item id="tree-table" docUrlSuffix="gui_TreeTable"/>
        </menu>
    </menu>

</sample-menu-config>
