<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        class="com.haulmont.sampler.web.ui.containers.htmlbox.simple.HtmlBoxFrame">
    <layout spacing="true">
        <htmlBox id="pictureHolder"
                 height="100%"
                 stylename="picture-holder"
                 template="html-layout"
                 width="100%">
            <vbox id="fanFirst"
                  stylename="fan fan-normal"/>
            <vbox id="fanSecond"
                  stylename="fan fan-normal"/>
            <vbox id="fanThird"
                  stylename="fan fan-normal"/>
            <vbox id="fanFourth"
                  stylename="fan fan-normal"/>
            <button id="speedDown"
                    caption="Slow"
                    invoke="speedDown"
                    stylename="speed-down"/>
            <button id="speedNormal"
                    caption="Normal"
                    invoke="speedNormal"
                    stylename="speed-normal"/>
            <button id="speedUp"
                    caption="Fast"
                    invoke="speedUp"
                    stylename="speed-up"/>
            <button id="moveUp"
                    caption="+"
                    invoke="moveUp"
                    stylename="move-up"/>
            <button id="moveDown"
                    caption="-"
                    invoke="moveDown"
                    stylename="move-down"/>
            <vbox id="waterLevel"/>
        </htmlBox>
    </layout>
</window>
