<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        class="com.haulmont.sampler.web.ui.charts.pie.donut.DonutChartFrame"
        xmlns:chart="http://schemas.haulmont.com/charts/charts.xsd">
    <layout>
        <chart:pieChart id="donutChart"
                        height="100%"
                        innerRadius="60%"
                        labelRadius="5"
                        labelText="[[title]]"
                        radius="42%"
                        titleField="title"
                        valueField="value"
                        width="100%"/>
    </layout>
</window>
